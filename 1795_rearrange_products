select product_id, 
  'store1' store,
  store1 price
from product
where store1 is not null
union all
select product_id, 
  'store2' store,
  store2 price
from product
where store2 is not null
union all
select product_id, 
  'store3' store,
  store3 price
from product
where store3 is not null
